{
    "name": "wdevkit/laravel-admin",
    "description": "Admin scafolding for Laravel",
    "keywords": [
        "laravel",
        "admin",
        "scafolding"
    ],
    "homepage": "https://github.com/wdevkit/laravel-admin",
    "license": "MIT",
    "authors": [
        {
            "name": "Gilmar Pereira",
            "email": "wdarking@gmail.com",
            "homepage": "https://github.com/wdarking",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "orchestra/testbench": "^5.0",
        "phpunit/phpunit": "^9.3",
        "psalm/plugin-laravel": "^1.2",
        "vimeo/psalm": "^3.11"
    },
    "autoload": {
        "psr-4": {
            "Wdevkit\\Admin\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Wdevkit\\Admin\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Wdevkit\\Admin\\AdminServiceProvider"
            ],
            "aliases": {
                "Admin": "Wdevkit\\Admin\\AdminFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
